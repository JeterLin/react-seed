<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <div id="root"></div>
    <% var vendorName = Object.keys(compilation.assets).filter(function(name){return name.indexOf('vendor')!==-1 && /\.js$/.test(name);})[0] %>
    <% if(vendorName && Object.keys(compilation.assets).indexOf('app')!==-1) { %><script id="vendors" src="<%= vendorName %>"></script><% } %>
</body>
</html>